<%@ page contentType="text/html; charset=UTF-8" %><%@ taglib uri="/WEB-INF/struts-bean.tld" prefix="bean" %><%@ taglib uri="/WEB-INF/struts-html.tld" prefix="html" %><%@ taglib uri="/WEB-INF/struts-logic.tld" prefix="logic" %><%@ taglib uri="/WEB-INF/struts-tiles.tld" prefix="tiles" %>
共计<bean:write name="portlet.tag.pager" property="count"/>项内容<logic:notEqual value="1" name="portlet.tag.pager" property="pageCount">，当前为<bean:write name="portlet.tag.pager" property="nextPage"/>/<bean:write name="portlet.tag.pager" property="pageCount"/>页</logic:notEqual><logic:present name="portlet.tag.first">，<a href="<bean:write name="portlet.tag.first"/>" target="_self">首页</a></logic:present><logic:present name="portlet.tag.prev">，<a href="<bean:write name="portlet.tag.prev"/>" target="_self">上一页</a></logic:present><logic:present name="portlet.tag.next">，<a href="<bean:write name="portlet.tag.next"/>" target="_self">下一页</a></logic:present><logic:present name="portlet.tag.last">，<a href="<bean:write name="portlet.tag.last"/>" target="_self">末页</a></logic:present>
<logic:notEqual value="1" name="portlet.tag.pager" property="pageCount">，跳转到 <input name="pagenum" value="1" id="pagenum" style="width:35px;" onblur="checkPageNum(this,<bean:write name="portlet.tag.pager" property="pageCount"/>)"/>&nbsp;<button type="button" onclick="gotoPage(this.parentNode.children['pagenum'],'<bean:write name="portlet.tag.URLPattern" filter="false"/>'+'&pageNo=')" style="font-size:12px;width:50px;height:25px;">跳转</button></logic:notEqual>
